<div class="member" style="border-bottom:1px solid #b5b5b5;padding-bottom:20px;">

<h2 class="title">
{{#if meta.hasChildren}}
<a href="{{{meta.fullname}}}.html">
{{/if}}
{{meta.name}}: {{doclet.type}}
{{#if meta.hasChildren}}
</a>
{{/if}}
</h2>

<div class="description">
{{{doclet.description}}}
</div>

{{#if meta.default}}
Default: <code>{{{meta.default}}}</code><br/>
{{/if}}

{{#if doclet.samples}}
<br/>
<br/>
<h4>Try it</h4>
<ul>
{{#each doclet.samples}}
<li><a href="#{{@this}}">{{@key}}</a></li>
{{/each}}
</ul>
{{/if}}

{{#if doclet.see}}

{{/if}}

{{#if meta.filename}}
<br/>
<i>Defined in <a href="https://github.com/highcharts/highcharts/blob/{{meta.commit}}/{{meta.filename}}#L{{meta.line}}">{{meta.filename}}:{{meta.line}}</a></i>
{{/if}}

</div>