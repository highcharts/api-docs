<div class="member" style="border-bottom:1px solid #b5b5b5;padding-bottom:20px;">

<h2 class="title">
{{#if node.meta.hasChildren}}
<a href="{{{node.meta.fullname}}}.html">
{{/if}}
{{node.meta.name}}: {{doclet.type}}
{{#if node.meta.hasChildren}}
</a>
{{/if}}
</h2>

<div class="description">
{{{node.doclet.description}}}
</div>

{{#if node.doclet.extends}} 
<div>
Extends: {{node.doclet.extends}}
</div>
{{/if}}

{{#if node.meta.inheritedFrom}}
<div>
Inherited from {{node.meta.fullname}}
</div>
{{/if}}

{{#if node.doclet.since}}
<div>
Since {{node.doclet.since}}
</div>
{{/if}}

{{#if node.meta.default}}
Default: <code>{{{node.meta.default}}}</code><br/>
{{/if}}


{{#if node.doclet.samples}}
<br/>
<br/>
<h4>Try it</h4>
<ul>
{{#each node.doclet.samples}}
<li><a href="#{{@this}}">{{@key}}</a></li>
{{/each}}
</ul>
{{/if}}

{{#if node.doclet.see}}

{{/if}}

{{#if node.meta.filename}}
<br/>
<i>Defined in <a href="https://github.com/highcharts/highcharts/blob/{{version.commit}}/{{node.meta.filename}}#L{{node.meta.line}}">{{node.meta.filename}}:{{node.meta.line}}</a></i>
{{/if}}

</div>