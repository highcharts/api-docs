<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{{#if node.meta.fullname}}{{node.meta.fullname}} | {{/if}}{{product}} {{platform}} API Reference</title>
        <link rel="stylesheet" href="style.css" type="text/css">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

        <link rel="apple-touch-icon" sizes="57x57" href="apple-touch-icon-57x57.png">
        <link rel="apple-touch-icon" sizes="114x114" href="apple-touch-icon-114x114.png">
        <link rel="apple-touch-icon" sizes="72x72" href="apple-touch-icon-72x72.png">
        <link rel="apple-touch-icon" sizes="144x144" href="apple-touch-icon-144x144.png">
        <link rel="apple-touch-icon" sizes="60x60" href="apple-touch-icon-60x60.png">
        <link rel="apple-touch-icon" sizes="120x120" href="apple-touch-icon-120x120.png">
        <link rel="apple-touch-icon" sizes="76x76" href="apple-touch-icon-76x76.png">
        <link rel="apple-touch-icon" sizes="152x152" href="apple-touch-icon-152x152.png">
        <link rel="apple-touch-icon" sizes="180x180" href="apple-touch-icon-180x180.png">
        <link rel="icon" type="image/png" href="favicon-192x192.png" sizes="192x192">
        <link rel="icon" type="image/png" href="favicon-160x160.png" sizes="160x160">
        <link rel="icon" type="image/png" href="favicon-96x96.png" sizes="96x96">
        <link rel="icon" type="image/png" href="favicon-16x16.png" sizes="16x16">
        <link rel="icon" type="image/png" href="favicon-32x32.png" sizes="32x32">

        <meta name="searchboost" content="{{searchBoost}}">
        <meta name="title" content="{{#if node.meta.fullname}}{{node.meta.fullname}}{{else}}{{product}} {{platform}} API Reference{{/if}}">
        <script type="text/javascript" src="api.js"></script>
    </head>
    <body>

        <div class="header">
            <div class="row row-1">
                <div id="sidebar-link" class="cell sidebar-link">
                    <a href="#"><i class="fa fa-bars" aria-hidden="true"></i></a>
                </div>
                <div id="highcharts-logo" class="cell">
                    <a href="http://www.highcharts.com" title="Highcharts Home Page">
                        <img alt="Highcharts Home Page" src="highcharts.svg" border="0">
                    </a>
                </div>
                <div id="page-title" class="cell">
                    <h1>{{platform}} API Reference</h1>
                </div>

            </div>
            <div class="row row-2">
                {{#if toc}}
                    <div id="product-selector" class="menu" expanded="false">
                        <a href="#" class="dropdown-link">{{product}} <i class="fa fa-chevron-down" aria-hidden="true"></i></a>
                        <div class="dropdown-menu">
                            <ul>
                                {{#each toc}}
                                <li class="product {{#if active}}highlighted{{/if}}">
                                    <a id="{{@key}}-link" href="{{#if versions.current}}{{versions.current}}{{else}}#{{/if}}">{{displayName}}</a>
                                </li>
                                {{/each}}
                            </ul>
                        </div>
                    </div>
                    {{#if platforms }}
                    <div id="platform-selector" class="menu" expanded="false">
                      <a href="#" class="dropdown-link">{{platform}}<i class="fa fa-chevron-down" aria-hidden="true"></i></a>
                      <div class="dropdown-menu">
                        <ul>
                          <li class="group">
                              <ul class="group-list">
                                  {{#each platforms }}
                                  <li class="version">
                                      <a href="{{@this}}">{{@key}}</a>
                                  </li>
                                  {{/each}}
                              </ul>
                          </li>
                        </ul>
                      </div>
                    </div>
                    {{/if}}
                    <div id="version-selector" class="menu" expanded="false">
                        <a href="#" class="dropdown-link">v{{version}} <i class="fa fa-chevron-down" aria-hidden="true"></i></a>
                        <div class="dropdown-menu">
                            <ul id="version-selector-body">
                                {{#each toc}}
                                <li class="group">
                                    <p>{{displayName}}</p>
                                    <ul class="group-list">
                                        {{#each versions}}
                                        <li class="version">
                                            <a href="{{@this}}" onclick="hapi.switchVersion(this);">{{@key}}</a>
                                        </li>
                                        {{/each}}
                                    </ul>
                                </li>
                                {{/each}}
                            </ul>
                        </div>
                    </div>
                  {{#if includeClassReference }}
                    <div id="namespaces" class="menu">
                        <a href="../class-reference/namespaces.list">Namespaces</a>
                    </div>
                    <div id="classes" class="menu">
                        <a href="../class-reference/classes.list">Classes</a>
                    </div>
                  {{/if}}
                {{/if}}
                {{#if progLangs}}
                    <div id="prog-lang-selector" class="menu" expanded="false">
                        <a href="#" class="dropdown-link">Language: {{language}} <i class="fa fa-chevron-down" aria-hidden="true"></i></a>
                        <div class="dropdown-menu">
                            <ul>
                                {{#each progLangs}}
                                <li class="prog-lang">
                                    <a id="{{@key}}-link" href="{{#if current}}{{current}}{{else}}#{{/if}}">{{@key}}</a>
                                </li>
                                {{/each}}
                            </ul>
                        </div>
                    </div>
                {{/if}}
            </div>
        </div>

        <div class="wrapper">
            <div id="sidebar" class="navigation sidebar" expanded="false">
                <div id="search" class="search-wrapper">
                    <i class="fa fa-search"></i>
                    <!--input class="search" /-->
                    <input id="searchBox" type="text" placeholder="search" />
                    <ul class="results"></ul>
                </div>
                <div id="option-trees-wrapper">
                    <h3 id="options-header">Configuration options</h3>
                    <p>For initial declarative chart setup. View as <a href="tree.json">JSON</a>.</p>
                    <div class="global-options options-tree">
                        <code>{{product}}.setOptions({</code>
                        <div id="global-options" class="tree"></div>
                        <code>});</code>
                    </div>
                    <div class="options-tree">
                        <code>{{product}}.{{constr}}({</code>
                        <div id="options" class="tree">
                            {{#if node.meta.name}}
                                <div class="node expanded option-{{node.meta.name}} parent">
                                    <a class="title" href=".">
                                        {{node.meta.name}}:{{#if node.children}}<i class="fa fa-caret-right"></i>{{/if}}
                                    </a>
                                    {{#if node.children}}
                                        <span class="bracket start">{</span>
                                        <div class="children" style="max-height:none;">
                                    {{/if}}
                            {{/if}}
                            {{#if node.children}}
                                {{#each node.children}}
                                    <div class="node expanded option-{{this.shortName}} {{#if node.meta.name}}leaf{{else}}parent{{/if}}">
                                        <a class="title" href="{{this.name}}">
                                            {{this.shortName}}:{{#if this.node.children}}<i class="fa fa-caret-right"></i>{{/if}}
                                        </a>
                                        {{#if this.node.children}}
                                            <span class="bracket start">{</span>
                                            <div class="children" style="max-height:none;">
                                                {{#each this.node.children}}
                                                    <div class="node collapsed option-{{this.shortName}} leaf">
                                                        <a class="title" href="{{this.name}}">
                                                            {{this.shortName}}:{{#if this.node.children}}<i class="fa fa-caret-right"></i>{{/if}}
                                                        </a>
                                                        {{#if this.node.children}}
                                                            <span class="bracket start">{</span>
                                                            <span class="dots">...</span>
                                                            <span class="bracket end first">}</span>
                                                        {{/if}}
                                                    </div>
                                                {{/each}}
                                            </div>
                                            <span class="bracket end second">}</span>
                                        {{else}}
                                            <span class="default type-undefined">{{this.node.doclet.defaultvalue}}</span>
                                        {{/if}}
                                    </div>
                                {{/each}}
                            {{else}}
                                <span class="default type-undefined">{{node.doclet.defaultvalue}}</span>
                            {{/if}}
                            {{#if node.meta.name}}
                                    {{#if node.children}}
                                        </div>
                                        <span class="bracket end second">}</span>
                                    {{/if}}
                                </div>
                            {{/if}}
                        </div>
                        <code>});</code>
                    </div>
                </div>
                {{#if includeClassReference }}
                <div id="members-wrapper">
                    <h3>Members and properties</h3>
                    <p>For modifying the chart at runtime. See the <a href="/class-reference/classes.list">class reference</a>.</p>
                </div>
                {{/if}}
            </div>

            {{#if node.meta.fullname}}
                <div id="body" class="body loaded">
                    <div id="splashText">
                        <h1>Welcome to the <strong>{{product}} {{platform}}</strong> Options Reference</h1>
                        <p>These pages outline the chart configuration options, and the methods and properties of Highcharts objects.</p>
                        <p>Feel free to search this <a target="_blank" href="https://en.wikipedia.org/wiki/Application_programming_interface">API</a> through the search bar or the navigation tree in the sidebar.</p>
                    </div>
                    <div id="option-list">
                        <div class="option highlighted">
                            <h1 class="title">{{node.meta.fullname}}</h1>
                            <p class="description">{{{node.doclet.description}}}</p>
                         </div>
                    </div>
                </div>
            {{else}}
                <div id="body" class="body">
                    <div id="splashText">
                        <h1>Welcome to the <strong>{{product}} {{platform}}</strong> Options Reference</h1>
                        <p>These pages outline the chart configuration options, and the methods and properties of Highcharts objects.</p>
                        <p>Feel free to search this <a target="_blank" href="https://en.wikipedia.org/wiki/Application_programming_interface">API</a> through the search bar or the navigation tree in the sidebar.</p>
                    </div>
                    <div id="option-list"></div>
                </div>
            {{/if}}
        </div>

        <div class="footer">
            <p>Copyright &copy; 2017, Highsoft AS. All rights reserved.</p>
            <div id="footer-social" class="footer-social">
                <a href="https://www.facebook.com/Highcharts" title="Facebook" class="social-icon"><i class="fa fa-facebook"></i></a>
                <a href="https://twitter.com/Highcharts" title="Twitter" class="social-icon"><i class="fa fa-twitter"></i></a>
                <a href="http://www.linkedin.com/company/highsoft-solutions-as" title="LinkedIn" class="social-icon"><i class="fa fa-linkedin"></i></a>
                <a href="https://github.com/highcharts/highcharts" title="Github" class="social-icon"><i class="fa fa-github"></i></a>
            </div>

            <p>
                {{product}} v{{version}} -
                Generated from branch <a href="https://github.com/highcharts/highcharts/tree/{{versionProps.branch}}">{{versionProps.branch}}</a> (commit <a href="https://github.com/highcharts/highcharts/commit/{{versionProps.commit}}">{{versionProps.commit}}</a>), on {{date}}
            </p>
        </div>
        <script>
            var state = '{{{node.meta.fullname}}}',
                hasChildren = true,
                product = '{{{product}}}';

            {{#unless node.meta.hasChildren}}
            hasChildren = false;
            {{/unless}}
            hapi.createNavigation('#options', '#global-options', state, product);
            hapi.createBody('#body', state, hasChildren);
            hapi.initializeDropdowns('.menu', '.dropdown-link', '.body');
            hapi.initializeSidebar('.sidebar', '.sidebar-link', '.body');
            hapi.initializeSearchBar('.search', '.results', 'search.json');
            hapi.simulateHistory();

            // Populate version
            hapi.populateVersions();
            
        </script>
        <script type="text/javascript">
            var ss360Config = {
                siteId: "api.highcharts.com",
                searchBoxSelector: "#searchBox",
                searchButton: undefined,
                postSearchCallback: function () {
                    searchResultSearchCallback.search = '';
                },
                // Appearance
                animationSpeed: 300,
                autofocus: true,
                defaultCss: true,
                navigation: 'top',
                themeColor: '#3A3A4F',
                // Suggestions
                extraHtml: undefined,
                highlight: true,
                minChars: 1,
                noSuggests: '<i>Sorry, we have no suggestions for you.</i>',
                numSuggestions: 10,
                showImageSuggestions: false,
                showSearchSuggestions: true,
                // Results
                highlightQueryTerms: false,
                moreResultsButton: 'Show more results',
                moreResultsPagingSize: 10,
                noResultsText: '<i>Sorry, we have not found any entries for "#QUERY#".</i>',
                numResults: 10,
                searchResults: {'contentBlock':'#body'},
                searchResultsCaption: 'Search results for "#QUERY#"',
                showImageResults: false,
            };
            (function (window) {
                window.ss360Config.includeContentGroups = [
                    'Highcharts',
                    'Highstock',
                    'Highmaps',
                    'Class Reference',
                ]
            }(this));
        </script>
        <script src="https://sitesearch360.com/cdn/sitesearch360-v10.min.js"></script>

    </body>

</html>
